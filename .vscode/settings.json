{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.addMissingImports": "explicit"
  },
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "prettier.jsxSingleQuote": false,
  "prettier.trailingComma": "es5",
  "html.format.wrapAttributes": "force-expand-multiline",
  "prettier.arrowParens": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "Clubwize",
    "IMGS"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#20477e",
    "activityBar.background": "#20477e",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#19060e",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#20477e",
    "statusBar.background": "#163055",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#20477e",
    "statusBarItem.remoteBackground": "#163055",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#163055",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#16305599",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#163055"
}
